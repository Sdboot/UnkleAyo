{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)\\.(?:js|css|gif|jpg|jpeg|png|svg|ico|webp|woff|woff2|eot|ttf|otf)$",
      "destination": "/$1.$2"
    },
    {
      "source": "/(?!api)(.*)",
      "destination": "/index.html"
    }
  ]
}

